    "@angular/platform-browser": "~4.0.0",
    "@angular/platform-browser-dynamic": "~4.0.0",
    "@angular/router": "~4.0.0",
    "angular-in-memory-web-api": "~0.3.0",
    "body-parser": "^1.17.1",
    "bootstrap": "^3.3.7",
    "compression": "^1.6.2",
    "core-js": "^2.4.1",
    "debug": "^2.6.3",
    "debug-log": "^1.0.1",
    "express": "^4.15.2",
    "helmet": "^3.5.0",
    "jquery": "^3.2.1",
    "mongojs": "^2.4.0",
    "mongoose": "^4.9.8",
    "morgan": "^1.8.1",
    "ngx-bootstrap": "^1.6.6",
    "passport": "^0.3.2",
    "path": "^0.12.7",
    "rxjs": "5.0.1",
    "socket.io": "^1.7.3",
    "systemjs": "0.19.40",
    "zone.js": "^0.8.4"
